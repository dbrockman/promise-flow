    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.5.3",
    "babel": "6.5.2",
    "babel-eslint": "7.1.1",
    "coveralls": "2.11.15",
    "eslint": "3.14.0",
    "eslint-config-springworks": "7.0.7",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-mocha": "4.8.0",
    "eslint-plugin-springworks": "2.0.1",
    "istanbul": "0.4.5",
    "mocha": "3.2.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbrockman/promise-flow.git"
