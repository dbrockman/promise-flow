  },
  "devDependencies": {
    "@springworks/test-harness": "1.3.11",
    "babel": "5.8.29",
    "babel-eslint": "6.0.5",
    "coveralls": "2.11.9",
    "eslint": "1.10.3",
    "eslint-config-springworks": "5.0.0",
    "eslint-plugin-import": "0.13.0",
    "eslint-plugin-mocha": "1.1.0",
    "eslint-plugin-springworks": "1.1.3",
    "istanbul": "0.4.4",
    "mocha": "2.5.3"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbrockman/promise-flow.git"
